class Solution:
    def isPalindrome(self, x: int) -> bool:
        y=str(x)
        if x<0 :
            return 0
        elif len(y)==1:
            return 1
        else:
            for i in range(len(y)//2):
                if y[i]!=y[len(y)-1-i]: 
                    return 0 
            return 1       
